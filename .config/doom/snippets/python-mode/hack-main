import sys
import requests
import base64
import urllib3
import urllib.parse

urllib3.disable_warnings(urllib3.exceptions.InsecureRequestWarning)

def main():
    if len(sys.argv) != 2:
        print( "(+) usage %s <target>" % sys.argv[0])
        print( "(+) eg: %s target" % sys.argv[0])
        sys.exit(1)

    target = sys.argv[1]

if __name__ == '__main__':
    main()
